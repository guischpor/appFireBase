    "slug": "appFireBase",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "androidStatusBar": {
      "backgroundColor": "#f15625"
    },
    "version": "1.0.0",
